<Window x:Class="practwork2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:practwork2"
        mc:Ignorable="d"
        Title="practwork2" Height="450" Width="800">
    <StackPanel>
        <DockPanel>
            <TextBlock x:Name="LoginTextBlock" Text="Логин" Margin="10"/>
            <TextBox Margin="10"/>
        </DockPanel>
        <DockPanel>
            <TextBlock x:Name="PasswordTextBlock" Text="Пароль" Margin="10" Foreground="#CD5C5C"/>
            <TextBox Margin="10"/>
        </DockPanel>
        <Button Content="Войти" Margin="10" Click="Button_Click"/>
    </StackPanel>
</Window>
